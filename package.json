    "react-modal": "^3.16.3",
    "react-toastify": "^11.0.3",
    "react-tooltip": "^5.28.0",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
