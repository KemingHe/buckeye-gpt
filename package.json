    "@types/react-dom": "^18.3.6",
    "@types/react-modal": "^3.16.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "daisyui": "^4.12.24",
    "dotenv-safe": "^9.1.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.3",
    "realfavicon": "^0.4.18",
    "tailwindcss": "^3.4.17",
