    "@stackframe/stack": "^2.8.2",
    "ai": "^4.3.2",
    "canvas-confetti": "^1.9.3",
    "langchain": "^0.3.20",
    "next": "^15.2.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-markdown": "^9.1.0",
    "react-modal": "^3.16.3",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.28.0",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
